    <IncludePath>$(SolutionDir)\IDASDK\include;$(SolutionDir)\IDASDK\plugins\debugger;$(IncludePath)</IncludePath>
    <OutDir>$(SolutionDir)\IDASDK\bin\plugins\$(ProjectName)\$(Configuration)\</OutDir>
